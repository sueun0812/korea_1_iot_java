package chapter07;

// final 키워드
// : 변수, 메서드, 클래스에서의 사용법

class MyFinal {
	// 1. final 변수
	// : 상수를 지정하는 키워드
	// >> 재할당 불가능, 선언과 동시에 초기화
	// >> 변수명을 모두 대문자로 작성, 단어와 단어의 연결을 _기호로 연결
	final int FINAL_VALIABLE = 30;
	FINAL_VALIABLE = 10; 
}
public class D_Final {

	public static void main(String[] args) {
		// 

	}

}
